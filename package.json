{
    "name": "vscode-abt-script",
    "displayName": "ABT Script",
    "description": "Language for ABT CPM chemistry system",
    "version": "0.0.1",
    "publisher": "dave8927",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "abtscript",
            "aliases": ["ABT Script", "abtscript", "abt-script", "vscode-abt-script"],
			"firstLine": "\t#!vscode-abt-script",
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ABTscript",
            "scopeName": "source.abt",
            "path": "./syntaxes/vscode-abt-script.tmLanguage.json"
        }]
    }
}