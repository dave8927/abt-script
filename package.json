{
    "name": "vscode-abt-script",
    "displayName": "vscode-abt-script",
    "description": "Language for ABT CPM scripting",
    "version": "0.0.1",
    "publisher": "dave8927",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "abtscript",
            "aliases": ["ABT Script", "abtscript"],
            "extensions": [".txt"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "abtscript",
            "scopeName": "source.abt",
            "path": "./syntaxes/abtscript.tmLanguage.json"
        }]
    }
}